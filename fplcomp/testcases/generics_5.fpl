# Generic with generic argument

class Group<T>
    val array = new Array<T>(10)

fun main()
    val b = new Group<Group<Int>>()

    for i=0 to 9
        b.array[i] = new Group<Int>()

    for i=0 to 9
        for j = 0 to 9
            b.array[i].array[j] = 2*i+j

    for i=0 to 9
        for j = 0 to 9
            print("%d ",b.array[i].array[j])
        print("\n")

    dumpMemorySystem()
